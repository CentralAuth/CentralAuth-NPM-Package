{
  "name": "centralauth",
  "version": "0.2.38",
  "description": "Official package for CentralAuth",
  "type": "module",
  "exports": {
    "./client": {
      "types": "./src/client.d.ts",
      "default": "./src/client.js"
    },
    "./server": {
      "types": "./src/server.d.ts",
      "default": "./src/server.js"
    },
    "./native": {
      "types": "./src/native.d.ts",
      "default": "./src/native.js"
    },
    "./types": {
      "types": "./src/types.d.ts",
      "default": "./src/types.js"
    },
    "./react.types": {
      "types": "./src/react.types.d.ts",
      "default": "./src/react.types.js"
    }
  },
  "scripts": {
    "build": "rm tsconfig.tsbuildinfo && openapi-ts && npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CentralAuth/CentralAuth-NPM-Package"
  },
  "homepage": "https://github.com/CentralAuth/CentralAuth-NPM-Package",
  "keywords": [
    "Authentication",
    "CentralAuth",
    "OAuth",
    "JWT"
  ],
  "author": "West IT",
  "license": "MIT",
  "dependencies": {
    "@hey-api/openapi-ts": "^0.87.1",
    "axios": "^1.13.2",
    "axios-cache-interceptor": "^1.8.3",
    "jose": "^6.1.1"
  },
  "peerDependencies": {
    "expo-crypto": ">=15.0.0",
    "expo-device": ">=8.0.0",
    "expo-linking": ">=8.0.0",
    "expo-secure-store": ">=15.0.0",
    "expo-web-browser": ">=15.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "expo-crypto": "^15.0.7",
    "expo-device": "^8.0.9",
    "expo-linking": "^8.0.8",
    "expo-secure-store": "^15.0.7",
    "expo-web-browser": "^15.0.9",
    "swr": "^2.3.6",
    "typescript": "^5.9.3"
  }
}